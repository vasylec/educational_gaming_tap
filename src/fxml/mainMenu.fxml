<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainMenuController">
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" layoutX="250.0" layoutY="148.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fx:id="play_button" fitHeight="75.0" fitWidth="100.0" onMousePressed="#play_button_pressed" onMouseReleased="#play_button_released" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../dependencies/play.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
                  <ImageView fitHeight="15.0" fitWidth="106.0" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fx:id="settings_button" fitHeight="65.0" fitWidth="100.0" onMousePressed="#settings_button_pressed" onMouseReleased="#settings_button_released" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../dependencies/settings.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
                  <ImageView fitHeight="15.0" fitWidth="107.0" pickOnBounds="true" preserveRatio="true" />
                  <ImageView fx:id="exit_button" fitHeight="90.0" fitWidth="100.0" onMousePressed="#exit_button_pressed" onMouseReleased="#exit_button_released" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../dependencies/exit.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
